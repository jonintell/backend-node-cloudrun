{
  "name": "backend-node-cloudrun",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy": "gcloud run deploy backend-node-cloudrun --source . --region us-central1 --allow-unauthenticated  --member='allUsers' --role='roles/run.invoker'"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.5.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
